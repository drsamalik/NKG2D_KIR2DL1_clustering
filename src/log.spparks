# species (note that in the C++ code indexing starts at 0 instead of 1)
add_species  l # i1 - unfed biofilm-resident bacteria
add_species	E # i2 - fed biofilm-resident bacteria (can replicate)
add_species	y # i3 - dead biofilm-resident bacteria
add_species	Y # i4 - unfed planktonic bacteria
add_species	N # i5 - fed planktonic bacteria
add_species	a # i6 - DNA network
add_species	A # i7 - nutrient particle
add_species	p # i8 - empty space (i.e. not a wall or free surface)
add_species	k # i9 - wall
add_species	v # i10 - wall
add_species	V # 11 - wall
add_species	P # i12 - wall
add_species	c # i13 - wall
add_species	C # i14 - wall
add_species	f # i15 - wall
add_species	W # i16 - wall

# AR+L_Aovercrowding of bacteria
# only activates when voxel is over-full by custom rule
add_rxn		0 local l a nbr 572.338502711 local A nbr
add_rxn		1 local A nbr 0.023 local l a nbr

# diffusion
add_rxn		2 local l nbr E 0.01 local nbr l E
add_rxn		3 local a nbr E 0.01 local nbr a E
add_rxn		4 local k nbr E 1.0 local nbr k E
add_rxn		5 local v nbr W 0.012397633664 local nbr v W

# AR+kinase
# bacteria do not twitch into full voxels by custom rule
# bacteria only move inside the DNA network by custom rule
add_rxn		6 local A k nbr 0.225186172253 local c nbr
add_rxn		7 local c nbr 53.1473593939 local A k nbr

# phopho AR
add_rxn		8 local c nbr 0.111830774889 local p k nbr

# ARp_vav
# this is only possible if there are already biofilm-resident bacteria or the eDNA network by custom rule
add_rxn		9 local p v nbr 266.159975537 local V nbr
add_rxn		10 local V nbr 0.0706191222802 local p v nbr

# ARp_vav_SHK
# rate multiplied by local & neighbor population
# voxels adjacent to a wall count their local population twice
# 5x lower rate in DNA network by custom rule
add_rxn		11 local V k nbr 61.2524439515 local C nbr
add_rxn		12 local C nbr 0.00545436963248 local V k nbr

# Vav phospho
add_rxn		13 local C nbr 0.0461142690474 local P k nbr

# dephospho
add_rxn		14 local P nbr 0.001 local V nbr
add_rxn		15 local p nbr 0.001 local A nbr

# dissociation complex phosphovav
add_rxn		16 local P nbr 1000.0 local p f nbr
add_rxn		17 local p f nbr 5.95628805825 local P nbr

#pvav diffusion
add_rxn		18 local f nbr W 0.0001 local nbr f W

# dephospho free pvav
add_rxn		19 local f nbr 0.419279012709 local v nbr

# Vav dependent movement 
add_rxn		20 local A nbr E 1.0 local nbr A E
add_rxn		21 local c nbr E 1.0 local nbr c E
add_rxn		22 local p nbr E 1.0 local nbr p E
add_rxn		23 local V nbr E 1.0 local nbr V E
add_rxn		24 local C nbr E 1.0 local nbr C E
add_rxn		25 local P nbr E 1.0 local nbr P E

# centripetal movement of Ar 
add_rxn		26 local A nbr E 1.0 local nbr A E
add_rxn		27 local c nbr E 1.0 local nbr c E
add_rxn		28 local p nbr E 1.0 local nbr p E
add_rxn		29 local V nbr E 1.0 local nbr V E
add_rxn		30 local C nbr E 1.0 local nbr C E
add_rxn		31 local P nbr E 1.0 local nbr P E

# dissociation of Ar complexes 
add_rxn		32 local c nbr 0.001 local l a k nbr
add_rxn		33 local p nbr 0.04513457378 local l a nbr
add_rxn		34 local V nbr 21.4404454024 local l a v nbr
add_rxn		35 local C nbr 0.00359170821592 local l a v k nbr
add_rxn		36 local P nbr 0.0171636057563 local l a v nbr


